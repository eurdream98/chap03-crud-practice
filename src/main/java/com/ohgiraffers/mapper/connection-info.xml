<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
    <comment/>
    <entry key="select1">
        SELECT
            *
        FROM
            department
        WHERE
            dept_id=?
    </entry>
    <entry key="select2">
        SELECT
            *
        FROM
            department
    </entry>
        <entry key="insert">
            INSERT  INTO
                department(dept_id,dept_title,location_id)
            VALUES
                (?,?,?)
        </entry>
    <entry key="update">
        UPDATE
            department
        SET
            dept_title=?,
            location_id=?
        WHERE
            dept_id=?
    </entry>
    <entry key="delete">
        DELETE FROM
            department
        WHERE
            dept_id=?
    </entry>
</properties>